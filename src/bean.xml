<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans.xsd 
  http://www.springframework.org/schema/context/ 
  http://www.springframework.org/schema/context/spring-context.xsd">
	<bean id="emp" class="com.chainsys.springproject.beans.Employee" />
	<bean id="emp2" class="com.chainsys.springproject.beans.Employee">
		<property name="id" value="2"></property>
		<property name="name" value="virat"></property>
	</bean>
	<bean id="c1" class="com.chainsys.springproject.beans.Customer"
		lazy-init="true" />
	<bean id="act" class="com.chainsys.springproject.beans.Actor"
		scope="prototype" />
	<bean id="flim" class="com.chainsys.springproject.beans.Movie">
		<constructor-arg type="java.lang.String"
			value="Arundhadhi" />
		<constructor-arg type="java.lang.String" value="Babu" />
	</bean>
	<bean id="sb1" class="com.chainsys.springproject.beans.Scoreboard"
		factory-method="createObject" scope="prototype" />
	<bean id="sb2" class="com.chainsys.springproject.beans.Scoreboard"
		factory-method="createObject" scope="prototype">
		<constructor-arg type="int" value="345" />
	</bean>
	<bean id="calender" class="com.chainsys.springproject.beans.Calender"
		factory-method="createCalender" factory-bean="cfactory" />
	<bean id="cfactory"
		class="com.chainsys.springproject.beans.CalenderFactory" />
	<bean id="app" class="com.chainsys.springproject.beans.Appointments"
		factory-method="createAppointments" factory-bean="cfactory" />
</beans>